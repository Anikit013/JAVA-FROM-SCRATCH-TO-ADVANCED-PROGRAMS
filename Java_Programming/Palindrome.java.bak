class Palindrome 
{
	public static void main(String[] args) 
	{
		int num=413;
		int temp=num;
		int rev=0;
		while(num>0)
		{
			int rem=num%10;
			rev=rev*10+rem;
			num=num/10;
		}
		if(rev==temp)
		{
			System.out.println("The number is a Palndrome");
		}
		else
		{
			System.out.println("The number is not a Palindrome");
		}
	}
}
